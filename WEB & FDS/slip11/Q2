import pandas as pd
import matplotlib.pyplot as plt
d = pd.read_csv('C:\\Users\\DELL\\Untitled Folder\\Iris.csv')
ax=plt.subplots(1,1,figsize=(10,8)) 
d['Species'].value_counts().plot.pie() 
plt.title("Iris Species %")
plt.show()






